<div id="viewport" class="contact">
    <div id="intro" data-type="background">

        <div class="row">
            <div class="container">
                <div class="header">
                    <h1>Contact</h1>
                </div>
                <div class="form-container">
                    <form role="form">
                      <div class="form-group">
                        <input placeholder="Name" type="text" class="form-control">
                      </div>
                      <div class="form-group">
                        <input placeholder="Email" type="email" class="form-control">
                      </div>
                      <div class="form-group">
                        <input placeholder="Subject" type="text" class="form-control">
                      </div>
                      <div class="form-group">
                        <textarea placeholder="Type your message here" class="form-control" rows="10"></textarea>
                      </div>
                      <div class="btn-container center-block">
                          <button type="submit" class="btn-blue">Submit</button>
                      </div>
                    </form>

                    <h2>Please call us on 01474 328917</h2>

                    <div class="table-container">
                    	<h4>Opening Times</h4>
                      	<table class="table">
                            <tbody>
                                <tr>
                                	<td class="no-border-left">Monday - Friday</td>
                                	<td class="no-border-right">07:30 to 18:00 (19:30 Fridays)</td>
                                </tr>
                                <tr class="no-border-right">
                                	<td class="no-border-left">Saturday</td>
                                	<td class="no-border-right">08:00 - 12:00</td>
                                </tr>
                                <tr class="no-border-right">
                                	<td class="no-border-left">Sunday</td>
                                	<td class="no-border-right">16:30 - 18:00 (drop off only)</td>
                            	</tr>
                            </tbody>
                        </table>
                        <h5>* We can, at times, accommodate earlier or later requests, if required.</h5>
                    </div>
                </div>
            </div>
